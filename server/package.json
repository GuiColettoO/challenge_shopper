{
  "name": "server-challenge-shopper",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "node dist/infrastructure/api-rest/server.js",
    "dev": "ts-node-dev --trace-warnings --respawn --transpile-only src/infrastructure/api-rest/server.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo:all",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:down:seed": "npx sequelize-cli db:seed:undo:all",
    "migrate:seed": "npx sequelize-cli db:seed:all"
  },
