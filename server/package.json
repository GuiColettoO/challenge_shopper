    "@types/jest": "^29.5.14",
    "@types/sequelize": "^4.28.20",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
